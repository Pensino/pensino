<!DOCTYPE html>
<html>
<head>
  <title>Pensino</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>


<div id='header'>
  <div id='app'>

    <div id='welcome'>
      Olá coordenador Paulo Schiavon |
      Preferências |
      <%= link_to t(:english), '/en' %> |
      <%= link_to t(:portuguese), '/pt-BR' %> |
      Sair
    </div>

    <h3 id='logo'>
      FATEC Jundiaí
    </h3>

    <div id='navigation'>
      <ul id='menu'>
        <li><%= link_to 'Courses', courses_path, :class => tab_class('Courses') %></li>
        <li><%= link_to 'Matters', matters_path, :class => tab_class('Matters')  %></li>
        <li><%= link_to 'Employees', employees_path, :class => tab_class('Employees') %></li>
        <li><%= link_to 'Students', students_path, :class => tab_class('Students') %></li>
        <li><%= link_to 'Expedients', expedients_path, :class => tab_class('Expedients') %></li>
        <li><%= link_to 'Lessons', lessons_path, :class => tab_class('Lessons') %></li>
        <li><%= link_to 'Registrations', registrations_path, :class => tab_class('Registrations') %></li>
      </ul>
    </div>
  </div>

</div>



<div id="wrapper">
  <div id="app">

    <div id="content">
      <%= yield %>
    </div>
  </div>
</div>

<div id="footer">
  <div id="app">

  </div>
</div>







</body>
</html>

