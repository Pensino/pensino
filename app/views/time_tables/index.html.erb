<h1>Listing time_tables</h1>

<table>
  <tr>
    <th>Grid</th>
    <th>Professor</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_tables.each do |time_table| %>
  <tr>
    <td><%= time_table.name %></td>
    <td><%= time_table.professor %></td>
    <td><%= time_table.start_date %></td>
    <td><%= time_table.end_date %></td>
    <td><%= link_to 'Show', time_table %></td>
    <td><%= link_to 'Edit', edit_time_table_path(time_table) %></td>
    <td><%= link_to 'Destroy', time_table, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

